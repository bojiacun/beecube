<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.winkt.modules.app.mapper.AppMapper">
    <select id="selectPageJoinAppModule" resultType="cn.winkt.modules.app.entity.App">
        select a.*,m.name as module_name from beecube_apps a left join beecube_modules m on m.id=a.module_id
    </select>
</mapper>